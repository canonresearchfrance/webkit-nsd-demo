            description {
               state: "default" 0.0;
//               min: 16 16;
               rel1 {
                  to_x:     "elm.swallow.icon";
                  relative: 1.0  0.0;
                  offset:   5    4;
               }
               rel2 {
                  to_x:     "elm.swallow.end";
                  relative: 0.0  0.7;
                  offset:   -5 -1;
               }
